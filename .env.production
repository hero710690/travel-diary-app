# Production Environment Configuration

# Database Configuration
DATABASE_TYPE=dynamodb

# AWS Configuration
AWS_REGION=us-east-1
USERS_TABLE=travel-diary-prod-users
TRIPS_TABLE=travel-diary-prod-trips
SESSIONS_TABLE=travel-diary-prod-sessions

# Application Configuration
ENVIRONMENT=production
DEBUG=false

# JWT Configuration (will be set via SSM Parameter Store)
# SECRET_KEY=your-production-secret-key

# API Configuration
API_V1_STR=/api/v1
PROJECT_NAME=Travel Diary API
VERSION=1.0.0

# CORS Configuration (will be updated with actual domain)
BACKEND_CORS_ORIGINS=["https://your-domain.com"]

# External APIs (will be set via SSM Parameter Store)
# GOOGLE_MAPS_API_KEY=your-google-maps-api-key
